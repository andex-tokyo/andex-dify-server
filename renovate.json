{
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "kubernetes/dify-deployment.yaml"
      ],
      "matchStrings": [
        "image: langgenius/dify-web(?<currentValue>.*)",
        "image: langgenius/dify-api(?<currentValue>.*)"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "langgenius/dify",
      "versioningTemplate": "semver"
    }
  ],
  "assignees": [
    "andex-tokyo"
  ]
}
